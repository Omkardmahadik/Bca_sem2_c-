#include<iostream>
using namespace std;
class employee
{
	private:
		int id,age,sal;
		char nam[10];
	
	public:
		void insert()
		{
			cout<<"Enteremployeename \n";
			cin>>nam;
			cout<<"Enteremployeeid \n";
			cin>>id;			
			cout<<"Enteremployeeage \n";
			cin>>age;
			cout<<"Enteremployeesalary \n";
			cin>>sal;
		}
		void disp()
		{
			cout<<"nam="<<nam;
			cout<<"id="<<id;
			cout<<"age="<<age;
			cout<<"sal="<<sal;
			

		}
		
};
main()
{
	employee e1;
	e1.insert();
	e1.disp();
}
